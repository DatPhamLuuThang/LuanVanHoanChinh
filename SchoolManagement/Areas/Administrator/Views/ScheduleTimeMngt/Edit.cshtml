@model CoreEntities.SchoolMgntModel.ScheduleTime

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input asp-for="Id" hidden>
<input asp-for="IsActive" hidden/>
<input asp-for="CreatedBy" hidden/>
<input asp-for="CreatedAt" hidden/>
<input asp-for="UpdatedBy" hidden/>
<input asp-for="UpdatedAt" hidden/>

<div class="form-group">
    <label asp-for="LessonName" class="control-label"></label>
    <input asp-for="LessonName" class="form-control"/>
    <span asp-validation-for="LessonName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="StartTime" class="control-label"></label>
    <input asp-for="StartTime" class="form-control"/>
    <span asp-validation-for="StartTime" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="EndTime" class="control-label"></label>
    <input asp-for="EndTime" class="form-control"/>
    <span asp-validation-for="EndTime" class="text-danger"></span>
</div>


