</div>
<div class="form-group">
    <label asp-for="EndIn" class="control-label"></label>
    <input asp-for="EndIn" class="form-control"/>
    <span asp-validation-for="EndIn" class="text-danger"></span>
</div>

<div class="form-group">
    <label asp-for="ExamWeek" class="control-label"></label>
    <input asp-for="ExamWeek" class="form-control"/>
    <span asp-validation-for="ExamWeek" class="text-danger"></span>
</div>
