@model CoreEntities.SchoolMgntModel.Semester

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input asp-for="Id" hidden>
<input asp-for="IsActive" hidden/>
<input asp-for="CreatedBy" hidden/>
<input asp-for="CreatedAt" hidden/>
<input asp-for="UpdatedBy" hidden/>
<input asp-for="UpdatedAt" hidden/>

<div class="form-group">
    <label asp-for="NameSemester" class="control-label"></label>
    <input asp-for="NameSemester" class="form-control"/>
